{
  "name": "honeypot",
  "version": "1.0.0",
  "description": "A custom dashboard for viewing/labeling content.",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build": "rm -rf server && cp -r src/ server/ && flow-remove-types src/ -d server/ --quiet",
    "flow": "flow",
    "buildandrunprod": "npm run build && npm run flow && node server/app.js --use_prod --debug",
    "prod-watch": "nodemon --signal SIGTERM --watch src/ --exec 'npm run buildandrunprod -- ' -- ",
    "buildandrunlocal": "npm run build && npm run flow && node server/app.js --use_local --debug",
    "local-watch": "nodemon --signal SIGTERM --watch src/ --exec 'npm run buildandrunlocal -- ' -- "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clondono/laCocina.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clondono/laCocina/issues"
  },
  "homepage": "https://github.com/clondono/laCocina#readme",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "antd": "^4.6.5",
    "aws-sdk": "^2.805.0",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "bs58": "^4.0.1",
    "csv": "^5.3.2",
    "express": "^4.17.1",
    "flow-typed": "^2.6.2",
    "generate-password": "^1.5.1",
    "hive-serving-request": "git+https://github.com/chatous/hive-serving-request.git",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-qs": "^3.0.0",
    "koa-router": "^10.0.0",
    "koa-send": "^5.0.1",
    "minimist": "^1.2.5",
    "path": "^0.12.7",
    "pg-promise": "^10.8.1",
    "process": "^0.11.10",
    "qs": "^6.9.4",
    "uuid-base62": "^0.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "eslint": "^7.14.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.138.0",
    "flow-remove-types": "^2.138.0",
    "nodemon": "^2.0.6",
